{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "description": "Extension that shows how addicted are you to internet",
   "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png" 
   },
   "browser_action": {
    "default_title": "Internet Junkie",
    "default_popup": "popup.html"
  },
   "manifest_version": 2,
   "name": "Internet Junkie",
   "permissions": [ 
       "tabs",
       "<all_urls>" 
   ],
    "version": "1.0"
}
