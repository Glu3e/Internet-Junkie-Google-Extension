{
   "background": {
      "persistent": true,
      "page": "background.html"
   },
   "description": "Internet Junkie - your own internet time tracker. Track how addicted you are to the internet",
   "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png" 
   },
   "browser_action": {
    "default_title": "Internet Junkie",
    "default_popup": "popup.html"
  },
   "manifest_version": 2,
   "name": "Internet Junkie",
   "short_name":"Internet Junkie",
   "permissions": [ 
       "tabs",
       "<all_urls>",
       "storage"
   ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "version": "1.1.3.5"
}
