{
   "background": {
      "persistent": true,
      "page": "/views/background.html"
   },
   "name": "Internet Junkie",
   "description": "Internet Junkie - your own internet time tracker. Track how much time you spend on the websites daily.",
   "icons": {
    "48": "/images/icon48.png",
    "128": "/images/icon128.png" 
   },
   "browser_action": {
    "default_title": "Internet Junkie",
    "default_popup": "/views/popup.html"
  },
   "manifest_version": 2,
   "short_name":"Internet Junkie",
   "web_accessible_resources": ["images/default_icon.png"],
   "permissions": [ 
       "tabs",
       "<all_urls>",
       "storage"
   ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "version": "1.1.3.6"
}
